{
  "name": "@36node/auth",
  "version": "1.0.0",
  "description": "36node auth service",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:docker": "docker buildx build --platform=linux/amd64 --push -t harbor.36node.com/36node/auth:main .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "gen:sdk": "ts-node scripts/gen-sdk.ts -o ./sdk",
    "mock": "node bin/mock.js",
    "start": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --passWithNoTests --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "github-secret": "ts-node scripts/github-action-secret.ts"
  },
  "dependencies": {
    "@alicloud/sms-sdk": "^1.1.6",
    "@faker-js/faker": "^8.3.1",
    "@nestjs/bull": "^10.1.1",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^9.4.3",
    "@nestjs/core": "^9.4.3",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.3.9",
    "@nestjs/mongoose": "^10.0.6",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/swagger": "^7.3.1",
    "axios": "^1.7.2",
    "bull": "^4.13.0",
    "cache-manager": "^5.6.1",
    "cache-manager-redis-yet": "^5.1.1",
    "captcha.gif": "^1.0.11",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.10",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "husky": "^8.0.3",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "memcache-pp": "^0.3.3",
    "mongodb": "6.2.0",
    "mongodb-memory-server": "^9.1.5",
    "mongoose": "^8.0.3",
    "mongoose-autopopulate": "^1.1.0",
    "mongoose-hidden": "^1.9.1",
    "mqtt": "^5.3.3",
    "nanoid": "^3.3.7",
    "nodemailer": "^6.9.13",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "path-to-regexp": "0.1.7",
    "postmark": "^4.0.2",
    "redis": "^4.6.14",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "source-map-support": "^0.5.20",
    "to-data-view": "^2.0.0"
  },
  "devDependencies": {
    "@36node/openapi-ts-gen": "^0.2.9",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@octokit/core": "^5.1.0",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.1",
    "@types/lodash": "^4.14.202",
    "@types/node": "18.16.12",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "commander": "^11.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "29.5.0",
    "libsodium-wrappers": "^0.7.13",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "~5.1.0"
  },
  "jest": {
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1",
      "test/(.*)": "<rootDir>/test/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.(spec|test)\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
